{
    "title": "胡子垚Java 学习笔记",
    "description": "学会总结和记录，以史为鉴",
    "author": "huziyao",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
      "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null,
            "github": "https://github.com/huziyao/JavaNote"
        },
        "sidebar": {
            "关于我的": "http://huziyao.live/about",
            "我的个人博客": "http://huziyao.live"
        }
    },
    "plugins": [
        "github@^2.0.0",
        "edit-link@^2.0.2",
        "anchors@^0.7.1",
        "include-codeblock@^3.0.2",
        "splitter@^0.0.8",
        "tbfed-pagefooter@^0.0.1",
        "expandable-chapters-small@^0.1.7",
        "anchor-navigation-ex@0.1.8",
        "multipart",
        "ancre-navigation",
        "code",
        "back-to-top-button",
        "copy-code-button",
        "-lunr", "-search", "search-pro",
        "advanced-emoji",
        "flexible-alerts",
        "lightbox"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "github": {
            "url": "https://github.com/huziyao/JavaNote"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © huziyao 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "edit-link": {
            "base": "https://github.com/huziyao/JavaNote/edit/master",
            "label": "Edit This Page"
        },
        "anchor-navigation-ex": {
            "isRewritePageTitle": false,
            "tocLevel1Icon": "fa fa-hand-o-right",
            "tocLevel2Icon": "fa fa-hand-o-right",
            "tocLevel3Icon": "fa fa-hand-o-right"
        },
        "flexible-alerts": {
              "style": "callout",
              "comment": {
              "label": "Comment",
              "icon": "fa fa-comments",
              "className": "info"
      }
    }


    }
}
